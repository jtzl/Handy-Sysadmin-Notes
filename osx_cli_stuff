restarting vnc:
	launchctl stop com.apple.ScreenSharing.server
	launchctl start com.apple.ScreenSharing.server


networksetup -getdnsservers Wi-Fi

networksetup listallnetworkservices

scutils --dns


relaunch dock:
	killall -KILL Dock
	
         finder:
	killall -KILL Finder

         desperate times...
        killall -KILL SystemUIServer
